                  <h2 class="card-header">Contactanos</h2>
                  <div class="card-body">
                      <span class="card-text">
                          <form>
                          <div class="mb-3">
                                  <label For="nameContact" class='form-label'>Nombres *</label>
                                  <input
                                  class='form-control'
                                      id="nameContact"
                                      name="nameContact"
                                      type="text"
                                      required
                                  />
                              </div>
                          <div class="mb-3">
                                  <label For="email" class='form-label'>Correo Electronico *</label>
                                  <input
                                  class='form-control'
                                      id="email"
                                      name="email"
                                      type="email"
                                      required
                                  />
                              </div>
                              <div class="mb-3">
                                  <label For="numberContact" class='form-label'>Numero de celular *</label>
                                  <input
                                      class='form-control'
                                      id="password"
                                      name="numberContact"
                                      type="int"
                                      required
                                  />
                              </div>
                              <button  type="submit" class="btn btn-primary">
                              Continuar
                          </button>
                      </form>
